{
  "name": "command-pad",
  "productName": "Command Pad",
  "version": "0.1.2",
  "description": "GUI for running terminal commands.",
  "main": "src/node/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "electron": "cross-env NODE_ENV=development electron ./",
    "electron:dist": "electron ./app",
    "pkg:osx": "electron-packager ./build",
    "start": "node ./tools/server.js",
    "build": "node ./tools/build.js",
    "test": "node ./tools/run_test.js app",
    "test:app": "node ./tools/run_test.js app",
    "build:test": "node ./tools/build_test_server.js",
    "pack": "build --dir",
    "dist": "npm run build && build"
  },
  "build": {
    "compression": "normal",
    "appId": "com.webows.commandpad",
    "dmg": {
      "icon": "build/icon.icns"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "identity": "Pei Wang (ZUQ2GPKPP2)"
    },
    "win": {
      "target": "nsis",
      "iconUrl": "https://github.com/supnate/command-pad"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "babel-preset-stage-0"
    ],
    "plugins": [
      "lodash",
      [
        "module-resolver",
        {
          "alias": {
            "src": "./src",
            "features": "./src/features"
          }
        }
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "index.js",
      "src/**/*.js",
      "tools/cli/*.js"
    ],
    "exclude": [
      "tools/**/templates",
      ".tmp"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": false,
    "sourceMap": false,
    "instrument": true
  },
  "rekit": {
    "studioPort": 6021
  },
  "webpackDevServerPort": 6076,
  "buildTestServerPort": 6077,
  "devDependencies": {
    "ansi-to-html": "^0.4.2",
    "antd": "^2.1.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.0.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-lodash": "^3.2.6",
    "babel-plugin-module-resolver": "^2.1.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "electron": "^1.4.3",
    "electron-builder": "^7.13.1",
    "electron-config": "^0.2.1",
    "electron-packager": "^8.1.0",
    "electron-rebuild": "^1.2.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-import-resolver-babel-module": "^2.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "express-history-api-fallback": "^2.2.1",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.4.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.14.2",
    "lodash-webpack-plugin": "^0.10.0",
    "memobind": "^0.5.0",
    "mocha": "^3.0.2",
    "mocha-webpack": "^0.6.0",
    "nock": "^8.0.0",
    "npm-run": "^4.1.0",
    "nyc": "^8.1.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.1.4",
    "redux-thunk": "^2.1.0",
    "rekit-core": "^2.2.5",
    "rekit-studio": "^2.2.6",
    "reselect": "^2.5.3",
    "shelljs": "^0.7.4",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dashboard": "0.1.8",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.3.3"
  },
  "dependencies": {
    "node-notifier": "^9.0.0",
    "node-pty": "^0.7.4",
    "node-version-compare": "^1.0.1",
    "request": "^2.75.0",
    "shallowequal": "^1.0.2"
  }
}
